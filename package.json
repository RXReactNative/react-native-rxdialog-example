{
    "name": "app",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "node node_modules/react-native/local-cli/cli.js start",
        "test": "node node_modules/jest/bin/jest.js --watch",
        "flow": "flow; test $? -eq 0 -o $? -eq 2",
        "clearFolder": "rm -rf docs",
        "web-build-prod": "set NODE_ENV=production && npm run clearFolder && ./node_modules/.bin/webpack -p --config ./web/webpack.prod.config.js",
        "web-build-dev-server": "NODE_ENV=development && npm run clearFolder && webpack-dev-server -d --config ./web/webpack.dev.config.js --port 3001 --inline --hot --colors  --history-api-fallback",
        "web-nginx-dev": "NODE_ENV=development && npm run clearFolder && ./node_modules/.bin/webpack -p --config ./web/webpack.dev.config.js"
    },
    "dependencies": {
        "react": "16.8.3",
        "react-native": "0.59.10",
        "react-art": "16.6.3",
        "react-dom": "16.6.3",
        "react-native-web": "^0.11.7",
        "react-native-linear-gradient": "^2.5.4",
        "react-native-web-linear-gradient": "^1.0.5",
        "react-native-rxdialog": "^1.0.4"
    },
    "devDependencies": {
        "babel": "6.23.0",
        "babel-core": "6.26.0",
        "babel-loader": "7.1.4",
        "file-loader": "^4.0.0",
        "react-native-md5": "^1.0.0",
        "babel-preset-react": "6.24.1",
        "babel-preset-es2015": "6.24.1",
        "babel-preset-stage-0": "6.24.1",
        "babel-preset-react-native": "4.0.1",
        "react-native-web-image-loader": "0.0.6",
        "html-webpack-plugin": "^3.2.0",
        "babel-plugin-module-resolver": "^3.1.1",
        "babel-plugin-react-native-web": "^0.9.9",
        "babel-plugin-react-transform": "3.0.0",
        "css-loader": "^2.0.2",
        "style-loader": "^0.23.1",
        "url-loader": "^1.1.2",
        "webpack": "^4.27.1",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.14",
        "webpack-merge": "^4.2.1",
        "babel-jest": "^24.9.0",
        "prettier": "^1.15.3",
        "jest": "^24.9.0",
        "metro-react-native-babel-preset": "^0.56.0",
        "react-test-renderer": "16.8.3"
    },
    "jest": {
        "preset": "react-native"
    }
}
