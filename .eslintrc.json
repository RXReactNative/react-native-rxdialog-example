{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "array-bracket-newline": ["error", "always"],
      "array-bracket-spacing": ["error", "always"],
      "array-element-newline": ["error", "always"],
      "comma-spacing": ["error", { "before": true, "after": true }],
      "indent": ["error", 2],
      "no-useless-computed-key": "error"
    }
}
